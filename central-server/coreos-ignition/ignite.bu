variant: fcos
version: 1.4.0
passwd:
  users:
    - name: etancik
      groups:
        - sudo
        - wheel
        - docker
      ssh_authorized_keys:
        - ssh-rsa  rsa-key-20221205
storage:
  links:
    - path: /etc/localtime
      target: ../usr/share/zoneinfo/Europe/Prague
  files:
    - path: /etc/hostname
      mode: 0644
      contents:
        inline: nuc
    - path: /etc/NetworkManager/system-connections/Wired connection 1.nmconnection
      mode: 0600
      contents:
        inline: |
          [connection]
          id=Wired connection 1
          type=ethernet
          interface-name=eno1
          [ipv4]
          address1=192.168.1.96/24,192.168.1.1
          dns=192.168.1.1;
          method=manual